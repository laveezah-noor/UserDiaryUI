    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.1*"/>
            <ColumnDefinition Width="0.4*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="0.1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="5"/>
            <RowDefinition Height="20" />
            <RowDefinition />
            <RowDefinition Height="5"/>
        </Grid.RowDefinitions>
        <TextBlock 
            Grid.Row="1" 
            Grid.Column="1">How old is your cat?</TextBlock>
        <TextBox 
            Name="Input"
            Grid.Row="1" 
            Grid.Column="2" 
            Height="15" 
            KeyDown="TextBox_KeyDown"
            ></TextBox>
        <StackPanel
            Grid.Column="1"
            Grid.Row="2"
            Grid.ColumnSpan="3"
            >
        <TextBlock
             
            Text="Your Cat is " 
            Name="Result"></TextBlock>
            <Image 
